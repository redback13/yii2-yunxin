{
    "name": "sndwow/yii2-yunxin",
    "description": "在Yii2中更好的使用云信组件",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yunxin"
    ],
    "require": {
        "php": "~8.0.0",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-httpclient": "^2.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "sndwow\\yunxin\\": "src"
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
